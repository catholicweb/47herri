"type": "mezak",
"notes": "{{ mass.notes }}",
"location": "{{ page.title | default: page.name | capitalize | replace: ".md", "" }}"
},
{%- endif -%}{%- endfor -%}{%- endif -%}
{%- if page.hiletak -%}{%- for hileta in page.hiletak -%}{%- if hileta.date -%}
{
"raw": "{{ hileta.date }}",
"type": "hiletak",
"notes": "{{ hileta.notes }}",
"location": "{{ page.title | default: page.name | capitalize | replace: ".md", "" }}"
},
{% endif %}{%- endfor -%}{% endif %}
{%- endfor -%}
{}]